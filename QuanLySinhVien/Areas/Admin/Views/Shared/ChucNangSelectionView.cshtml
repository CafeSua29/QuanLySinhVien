@model List<QuanLySinhVien.Models.ViewModel.ChucNangSelection>

<table>
    @{
        for (int i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>@Html.CheckBoxFor(item => item[i].IsSelected)</td>
                <td style="display:none;">@Html.HiddenFor(item => item[i].CN.MaCN)</td>
                <td>@Html.DisplayFor(item => item[i].CN.TenCN)</td>
            </tr>
        }
    }
</table>